<?xml version="1.0" encoding="UTF-8"?>
<!-- OPML generated by TwitterListOPML -->
<opml version="1.1">
	<head>
        <title>{{ title }}</title>
	</head>
<body>
    <outline text="{{ title }}" title="{{ title }}">
    {% for channel in channels %}
    <outline 
        text="{{ channel.name }}" 
        title="{{ channel.name }}" 
        type="rss" 
        version="RSS" 
        htmlUrl="{{ channel.link }}" 
        xmlUrl="{{ channel.rss_feed }}" />
    {% endfor %}
    </outline>
</body>
</opml>

